<Window x:Class="MemberDetailsProcessor.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MemberDetailsProcessor"
        mc:Ignorable="d"
        Title="E2W Converter" Height="410" Width="500">
    <Grid>
        <Button x:Name="upload" Content="Upload" HorizontalAlignment="Left" Margin="50,175,0,0" VerticalAlignment="Top" Width="100" Click="upload_Click" Height="33" />
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="33" Margin="155,175,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="162" />
        <Button x:Name="convert" Content="Convert" HorizontalAlignment="Left" Margin="348,175,0,0" VerticalAlignment="Top" Width="112" Height="33" Click="convert_Click" />
        <TextBlock x:Name="textBlock" HorizontalAlignment="Left" Margin="100,241,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" FontSize="16">
            <TextBlock.BindingGroup>
                <BindingGroup />
            </TextBlock.BindingGroup>
        </TextBlock>
    </Grid>
</Window>